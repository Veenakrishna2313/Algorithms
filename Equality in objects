function Address(street, city, zipcode)  {
   this.street=street,
   this.city=city,
    this.zipcode=zipcode
  }

const anotherAddress1= new Address("a", "b"," code");
const anotherAddress2= new Address("a", "b", "code");
// console.log(anotherAddress1);
// console.log(anotherAddress2);

areEqual=(address1, address2)=>{
return address1.street===address2.street &&
  address1.city===address2.city &&
  address1.zipcode===address2.zipcode;
     
}

console.log(areEqual(anotherAddress1, anotherAddress2));

areSame=(address1, address2)=>{
  return address1===address2;
}

console.log(areSame(anotherAddress1, anotherAddress2));
