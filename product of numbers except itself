/**
 * @param {number[]} nums
 * @return {number[]}
 */
var productExceptSelf = function(nums) {
    
    let len=nums.length;
      let product=1, output=[];
    if(len<1) return output;
    
    for(i=0;i<len;i++){
        output[i]=product*nums[i];
        product=output[i];
    }
    
    product=1;
    for(i=len-1;i>0;i--){     
        output[i]=output[i-1]*product;
        product=product*nums[i]          
    }   
    output[0]=product;

    return output;
    
};
